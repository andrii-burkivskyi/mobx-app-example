{
  "name": "app-generator",
  "version": "1.0.0",
  "description": "App generator",
  "main": "public/server/server/main.js",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start": "yarn build && electron ./public/server/server/main.js",
    "watch:server": "nest start --watch",
    "watch:client": "cross-env webpack-dev-server --hot --mode development --output-pathinfo --inline --source-map --config configs/client/webpack.config.js",
    "watch:electron": "tsc -p configs/electron/tsconfig.json -w",
    "start:server": "node index.js",
    "build": "yarn build:client && yarn build:electron",
    "build:server": "cross-env tsc -p configs/server/tsconfig.json",
    "build:client": "NODE_ENV=production cross-env webpack --mode production --config configs/client/webpack.config.js",
    "build:electron": "cross-env tsc -p configs/electron/tsconfig.json",
    "typeorm": "ts-node -P configs/server/tsconfig.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/server/configs/typeorm_config.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "typeorm:run": "npm run typeorm migration:run"
  },
  "keywords": [
    "Electron"
  ],
  "author": "Andrii Burkivskyi",
  "license": "MIT",
